{
    "schemaVersion": 1,
    "id": "${id}",
    "version": "${version}",
    "name": "${name}",
    "description": "${description}",
    "authors": [
        "${author}"
    ],
    "contact": {
        "homepage": "${source}",
        "sources": "${source}",
        "issues": "${source}/issues"
    },
    "license": "MIT",
    "icon": "assets/${id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            {
                "adapter": "kotlin",
                "value": "${group}.EntrypointKt::init"
            }
        ],
        "fabric-datagen": [
            {
                "adapter": "kotlin",
                "value": "${group}.DataGenerator"
            }
        ]
    },
    "mixins": [
        "${id}.mixins.json"
    ]
}
